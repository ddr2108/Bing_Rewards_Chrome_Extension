{
  "name": "Bing Rewards",
  "version": "3.1",
  "manifest_version": 3,
  "description": "Quickly get points for Bing Rewards",
  "browser_action": {
    "default_icon": "bing.png"
  },
  "background": {
    "scripts": ["rewards.js"]
  },
  "permissions": [
    "tabs",
    "background",
    "https://ajax.googleapis.com/",
    "http://www.bing.com/"
  ]
}
